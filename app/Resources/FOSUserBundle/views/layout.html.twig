{% extends '::layout/layout.html.twig' %}
{% block body %}

        {% block fos_user_content %}
        {% endblock fos_user_content %}
    {% for type, messages in app.session.flashbag.all() %}
        {% for message in messages %}
            <center>
                <div class="flash-{{ type }} alert alert-success" role="alert">
                    {{ message }}
                </div>
            </center>
        {% endfor %}
    {% endfor %}


{% endblock %}
